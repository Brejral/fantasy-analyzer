{
    "name": "fantasy-analyzer",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": " ng serve",
        "start:prod": "NODE_OPTIONS=--max-old-space-size=4096 ng serve --prod --base-href .",
        "start:hmr": " ng serve --hmr",
        "start:hmr:open": "NODE_OPTIONS=--max-old-space-size=4096 ng serve -o --hmr",
        "ng:ci:validation": "NODE_OPTIONS=--max-old-space-size=4096 npm run lint & npm run build:ci & npm run test:ci",
        "compodoc": "NODE_OPTIONS=--max-old-space-size=4096 ./node_modules/.bin/compodoc -p src/tsconfig.app.json --disableRoutesGraph",
        "build": "NODE_OPTIONS=--max-old-space-size=4096 ng build --aot",
        "build:ci": "NODE_OPTIONS=--max-old-space-size=4096 ng build --prod && node ./deployment/prod.js",
        "build:ci:qa": "NODE_OPTIONS=--max-old-space-size=4096 ng build --configuration qa && node ./deployment/qa.js",
        "build:ci:uat": "NODE_OPTIONS=--max-old-space-size=4096 ng build --configuration uat && node ./deployment/uat.js",
        "test": "NODE_OPTIONS=--max-old-space-size=4096 ng test",
        "test:ci": "NODE_OPTIONS=--max-old-space-size=4096 ng test --browsers ChromeHeadless --watch false --environment prod",
        "lint": "NODE_OPTIONS=--max-old-space-size=4096 ng lint",
        "e2e": "NODE_OPTIONS=--max-old-space-size=4096 ng e2e",
        "e2e:ci": "NODE_OPTIONS=--max-old-space-size=4096 ng e2e",
        "android": "NODE_OPTIONS=--max-old-space-size=4096 tns run android --bundle",
        "ios": "NODE_OPTIONS=--max-old-space-size=4096 tns run ios --bundle",
        "android:hmr": "NODE_OPTIONS=--max-old-space-size=4096 tns run android --hmr",
        "ios:hmr": "NODE_OPTIONS=--max-old-space-size=4096 tns run ios --hmr",
        "android:debug:hmr": "NODE_OPTIONS=--max-old-space-size=4096 tns debug android --hmr",
        "ios:debug:hmr": "NODE_OPTIONS=--max-old-space-size=4096 tns debug ios --hmr",
        "ios:build": "NODE_OPTIONS=--max-old-space-size=4096 tns build ios --bundle",
        "android:build": "NODE_OPTIONS=--max-old-space-size=4096 tns build android --bundle",
        "clean": "NODE_OPTIONS=--max-old-space-size=4096 rimraf **/*.component.css **/*.component.tns.css **/*.base.css app.css app.tns.css hooks dist platforms node_modules"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~8.2.4",
        "@angular/cdk": "^7.1.1",
        "@angular/common": "~8.2.4",
        "@angular/compiler": "~8.2.4",
        "@angular/core": "~8.2.4",
        "@angular/forms": "~8.2.4",
        "@angular/http": "^7.2.15",
        "@angular/material": "~7.1.1",
        "@angular/platform-browser": "~8.2.4",
        "@angular/platform-browser-dynamic": "~8.2.4",
        "@angular/router": "~8.2.4",
        "@angularclass/hmr": "~2.1.3",
        "@auth0/angular-jwt": "^2.1.0",
        "@ngrx/effects": "~7.0.0",
        "@ngrx/entity": "~7.0.0",
        "@ngrx/router-store": "~7.0.0",
        "@ngrx/store": "~7.0.0",
        "@ngrx/store-devtools": "~7.0.0",
        "cross-env": "^5.2.0",
        "ngrx": "^2.0.1",
        "rxjs": "~6.5.2",
        "tslib": "^1.10.0",
        "zone.js": "~0.9.1"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.803.2",
        "@angular/cli": "~8.3.2",
        "@angular/compiler-cli": "~8.2.4",
        "@angular/language-service": "~8.2.4",
        "@types/node": "~8.9.4",
        "@types/jasmine": "~3.3.8",
        "@types/jasminewd2": "~2.0.3",
        "codelyzer": "^5.0.0",
        "jasmine-core": "~3.4.0",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "~4.1.0",
        "karma-chrome-launcher": "~2.2.0",
        "karma-coverage-istanbul-reporter": "~2.0.1",
        "karma-jasmine": "~2.0.1",
        "karma-jasmine-html-reporter": "^1.4.0",
        "protractor": "~5.4.0",
        "ts-node": "~7.0.0",
        "tslint": "~5.15.0",
        "typescript": "~3.5.3"
    }
}
