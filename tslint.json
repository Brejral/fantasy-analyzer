{
				"defaultSeverity": "error",
				"rulesDirectory": [
								"node_modules/codelyzer"
				],
				"extends": [
								"tslint:recommended"
				],
				"rules": {
								"no-reference": false,
								"completed-docs": [
												false,
												{
																"methods": {
																				"privacies": [
																								"public",
																								"protected"
																				]
																},
																"interfaces": true,
																"types": true,
																"properties": {
																				"privacies": [
																								"public",
																								"protected"
																				]
																}
												}
								],
								"arrow-parens": false,
								"max-classes-per-file": false,
								"array-type": false,
								"interface-name": false,
								"only-arrow-functions": false,
								"no-empty": false,
								"typedef": [
												true,
												"call-signature",
												"variable-declaration",
												"property-declaration",
												"parameter",
												"member-variable-declaration"
								],
								"indent": [
												true,
												"tabs",
												4
								],
								"one-line": false,
								"semicolon": true,
								"quotemark": [
												true,
												"single"
								],
								"trailing-comma": false,
								"object-literal-sort-keys": false,
								"variable-name": [
												true,
												"ban-keywords",
												"check-format",
												"allow-leading-underscore"
								],
								"import-blacklist": [
												true,
												"ban-keywords",
												"check-format",
												"allow-leading-underscore",
												"rxjs/Rx"
								],
								"import-spacing": false,
								"space-before-function-paren": false,
								"max-line-length": [
												true,
												220
								]
				},
				"linterOptions": {
								"exclude": [
												"node_modules"
								]
				}
}